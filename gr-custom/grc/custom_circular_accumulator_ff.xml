<?xml version="1.0"?>
<block>
  <name>Circular Accumulator</name>
  <key>custom_circular_accumulator_ff</key>
  <category>[Custom]/Math Operators</category>
  <import>import custom</import>
  <make>custom.circular_accumulator_ff($modulus)</make>
  
  <param>
    <name>Modulus</name>
    <key>modulus</key>
    <type>float</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
